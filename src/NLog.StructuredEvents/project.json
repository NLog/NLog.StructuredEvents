{
  "version": "0.4.0",
  "description": "Parsing and rendering message templates",
  "authors": [ "Julian Verdurmen" ],
  "packOptions": {
    "summary": "NLog - Message template parser and renderer",
    "tags": [ "template", "parse", "render" ],
    "projectUrl": "http://nlog-project.org/",
    "licenseUrl": "http://raw.github.com/NLog/NLog.StructuredEvents/master/LICENSE",
    "requireLicenseAcceptance": false,
    "iconUrl": "http://nlog-project.org/N.png",
    "repository": {
      "type": "git",
      "url": "git://github.com/NLog/NLog.StructuredEvents"
    },
    "releaseNotes": "CLSCompliant (breaking change in Literal properties)"

  },

  "frameworks": {
    "netstandard1.0": {
      "imports": "dnxcore50",
      "buildOptions": {
        "define": [ "NETSTANDARD" ]
